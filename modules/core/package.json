{
  "name": "@hubble.gl/core",
  "version": "1.4.0",
  "description": "hubble.gl core library classes",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/hubble.gl"
  },
  "keywords": [
    "kepler.gl",
    "deck.gl",
    "webgl",
    "visualization"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "src",
    "dist",
    "dist.min.js"
  ],
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run build-bundle && npm run build-bundle -- --env.dev",
    "build-bundle": "ocular-bundle ./bundle.ts"
  },
  "dependencies": {
    "@loaders.gl/core": "^3.4.13",
    "@loaders.gl/video": "^3.4.13",
    "@loaders.gl/zip": "^3.4.13",
    "@math.gl/core": "^3.6.0",
    "@math.gl/web-mercator": "^3.6.2",
    "@probe.gl/log": "^4.1.0",
    "downloadjs": "^1.4.7",
    "popmotion": "9.3.1",
    "webm-writer": "^1.0.0"
  },
  "resolutions": {
    "@loaders.gl/video": "3.0.12"
  },
  "peerDependencies": {
    "@deck.gl/core": ">=8.3",
    "@luma.gl/core": ">=8.3",
    "@luma.gl/engine": ">=8.3"
  },
  "devDependencies": {
    "@deck.gl/core": "^8.9",
    "@luma.gl/core": "^8.5.0",
    "@luma.gl/engine": "^8.5.0",
    "@types/tape-catch": "^1.0.2"
  }
}
